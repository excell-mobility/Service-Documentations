{
	"swagger": "2.0",
	"info": {
		"description": "This service offers an API to upload GPS-position data. The uploaded data will be anonymized, map-matched and used for traffic data analysis.",
		"version": "1.0",
		"title": "ExCELL Tracking Service",
		"contact": {
			"name": "TU Dresden - Chair of Traffic Control and Process Automatisation",
			"url": "http://tu-dresden.de/vlp",
			"email": "henning.jeske@tu-dresden.de"
		}
	},
	"host": "excell.vkw.tu-dresden.de:20080",
	"basePath": "/",
	"tags": [
		{
			"name": "tracking-service-controller",
			"description": "Tracking Service Controller"
		}]
	,
	"paths": {
		"/TrackingService/getPositionData": {
			"get": {
				"tags": [
					"tracking-service-controller"]
				,
				"summary": "Get last tracking information for one or more IDs",
				"operationId": "getPositionDataUsingGET",
				"consumes": [
					"application/json"]
				,
				"produces": [
					"application/json",
					"*/*"]
				,
				"parameters": [
					{
						"name": "idList",
						"in": "query",
						"description": "idList",
						"required": true,
						"type": "array",
						"items": {
							"type": "string"
						},
						"collectionFormat": "multi"
					}]
				,
				"responses": {
					"200": {
						"description": "OK",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/SimpleGpsRecord"
							}
						}
					},
					"401": {
						"description": "Unauthorized"
					},
					"403": {
						"description": "Forbidden"
					},
					"404": {
						"description": "Not Found"
					}
				}
			}
		},
		"/TrackingService/putPositionData/{appId}/{id}": {
			"put": {
				"tags": [
					"tracking-service-controller"]
				,
				"summary": "Put Postion Data",
				"operationId": "putPostionDataUsingPUT",
				"consumes": [
					"application/json"]
				,
				"produces": [
					"application/json",
					"*/*"]
				,
				"parameters": [
					{
						"name": "appId",
						"in": "path",
						"description": "appId",
						"required": true,
						"type": "string"
					},
					{
						"name": "id",
						"in": "path",
						"description": "id",
						"required": true,
						"type": "string"
					},
					{
						"in": "body",
						"name": "gpsRecordList",
						"description": "gpsRecordList",
						"required": true,
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/FullGpsRecord"
							}
						}
					}]
				,
				"responses": {
					"200": {
						"description": "OK",
						"schema": {
							"type": "string"
						}
					},
					"201": {
						"description": "Created"
					},
					"401": {
						"description": "Unauthorized"
					},
					"403": {
						"description": "Forbidden"
					},
					"404": {
						"description": "Not Found"
					}
				}
			}
		}
	},
	"definitions": {
		"FullGpsRecord": {
			"type": "object",
			"properties": {
				"accuracy": {
					"type": "number",
					"format": "double"
				},
				"altitude": {
					"type": "number",
					"format": "double"
				},
				"bearing": {
					"type": "number",
					"format": "double"
				},
				"date": {
					"type": "string",
					"format": "date-time"
				},
				"latitude": {
					"type": "number",
					"format": "double"
				},
				"locationProvider": {
					"type": "string"
				},
				"longitude": {
					"type": "number",
					"format": "double"
				},
				"speed": {
					"type": "number",
					"format": "double"
				},
				"tmode": {
					"type": "string"
				}
			}
		},
		"SimpleGpsRecord": {
			"type": "object",
			"properties": {
				"date": {
					"type": "string",
					"example": "yyyy-MM-dd'T'HH:mm:ss"
				},
				"id": {
					"type": "string"
				},
				"latitude": {
					"type": "number",
					"format": "double"
				},
				"longitude": {
					"type": "number",
					"format": "double"
				}
			}
		}
	}
}
